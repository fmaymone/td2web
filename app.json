{
  "cron": [
    {
      "command": "rake team_diagnostics:deploy",
      "schedule": "*/5"
    },
    {
      "command": "rake team_diagnostics:remind",
      "schedule": "@hourly"
    },
    {
      "command": "rake team_diagnostics:complete",
      "schedule": "*/5"
    },
    {
      "command": "rake system:send_error_digest[30]",
      "schedule": "*/30"
    },
  ]
}
